{
  "$schema": "https://packageurl.org/schemas/purl-type-definition.schema-1.0.json",
  "$id": "https://packageurl.org/types/gitee-definition.json",
  "type": "gitee",
  "type_name": "Gitee",
  "description": "Gitee-based packages",
  "repository": {
    "use_repository": true,
    "default_repository_url": "https://gitee.com"
  },
  "namespace_definition": {
    "requirement": "required",
    "case_sensitive": true,
    "native_name": "user or organization",
    "note": "The namespace is the user or organization. It is case sensitive."
  },
  "name_definition": {
    "case_sensitive": true,
    "native_name": "repository name",
    "note": "The name is the repository name. It is case sensitive."
  },
  "version_definition": {
    "native_name": "commit or tag",
    "note": "The version is a commit or tag."
  },
  "examples": [
    "pkg:gitee/ThingsGateway/ThingsGateway@ec5ff0a07f8838f",
    "pkg:gitee/ThingsGateway/ThingsGateway@10.10.9.0"
  ]
}
