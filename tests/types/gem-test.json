      "test_group": "base",
      "test_type": "parse",
      "input": "pkg:gem/jruby-launcher@1.1.2?Platform=java",
      "expected_output": {
        "type": "gem",
        "namespace": null,
        "name": "jruby-launcher",
        "version": "1.1.2",
        "qualifiers": {
          "platform": "java"
        },
        "subpath": null
      },
      "expected_failure": false,
      "expected_failure_reason": null
    },
