          "build_type": "Debug",
          "compiler": "Visual Studio",
          "compiler.runtime": "MDd",
          "compiler.version": "16",
          "os": "Windows",
          "prev": "b429db8a0e324114c25ec387bfd8281f330d7c5c",
          "rrev": "93a82349c31917d2d674d22065c7a9ef9f380c8e",
          "shared": "True"
